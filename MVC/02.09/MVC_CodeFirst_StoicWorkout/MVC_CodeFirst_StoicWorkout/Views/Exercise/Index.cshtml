@model List<MVC_CodeFirst_StoicWorkout.Models.ViewModels.ExerciseViewModels.ExerciseListViewModel>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<partial name="_MessagePartial" />"

<table class=" table table-hover">
   <thead>
       <tr>
           <th>ID</th>
           <th>ExerciseName</th>
            <th>RepRange</th>
            <th>Sets</th>
            <th>RestTime</th>
            <th>Difficulty</th>
            <th>Equipment</th>
            <th>TargetMuscleGroup</th>
       </tr>
   </thead>
   <tbody>
       @foreach (var item in Model)
       {
           <tr>
               <td>@item.ID</td>
               <td>@item.ExerciseName</td>
               <td>@item.RepRange</td>
               <td>@item.Sets</td>
               <td>@item.RestTime</td>
               <td>@item.Difficulty</td>
               <td>@item.Equipment</td>
               <td>@item.TargetMuscleGroup</td>
           </tr>
        }

</table>

  
